package code.util;

public class Meat {
	
	private	String	title;
	private boolean	roasted;
	
	public	void	Meat(){
		
	}
	
	public	void	setTitle(String	title){
		this.title = title;
	}

	public	String	getTitle(){
		return	this.title;
	}
	
	public	void	setRoasted(boolean	roasted){
		this.roasted = roasted;
	}
	
	public	boolean	isRoasted(){
		return	this.roasted;
	}
}
